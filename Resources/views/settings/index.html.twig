{% extends '@HarmonyAdmin/default/layout.html.twig' %}

{% trans_default_domain 'HarmonyAdminBundle' %}

{% block content_title %}
  {% spaceless %}
    {{ 'settings.title'|trans|raw }}
  {% endspaceless %}
{% endblock %}

{% block main %}
  <div class="container-fluid">
    <div class="row">
      <div class="col-12">
        {{ form_start(form) }}
        {{ form_widget(form.settings) }}
        {{ form_end(form) }}
      </div>
    </div>
  </div>
{% endblock %}